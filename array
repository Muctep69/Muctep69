#include <iostream>
#include <ctime>

using namespase std;

int main()

{
   setlocale(0,"");
   srand(time(NULL));
     
   int n=0; max=0, j=0;
   int *arr=new int[n];
    
    cout<<"введите размер массива "<<endl;
    cin>>n;
    
    
     for (int i=0 ; i<n ; i++)
     
     {
      arr[i]=rand()%21 - 10;
       cout<<arr[i]<<"  ";
       
     }
      cout<<endl;
      
     for (int i=0 ; i<n ; i++)
     
     {
        if(arr[i]%5==0)
          {
            j++;
          
          }
     
     }
    for (int i=0 ; i<n ; i++)
    {
      if(arr[i]==0)
      {
       j--;
      }
    
    }
     cout<<"колличество элементов кратное 5 = "<<j<<endl;
     
     
     
     
     max=arr[0];
      for (int i=0 ; i<n ; i++)    
         {
         if(arr[i]>max)
         {
           max=arr[i];
         }
         
         }

      cout<<"max= "<<max<<endl;



}


